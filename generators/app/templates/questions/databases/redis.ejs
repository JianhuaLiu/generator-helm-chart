<% if (_.find(databases, { name: 'redis'})) { %>

# Redis Config
  - variable: config.redis.internal
    default: true
    description: ''
    type: boolean
    required: true
    label: 'redis internal'
    group: 'Redis Config'
  - variable: config.redis.host
    default: ''
    description: ''
    type: string
    required: false
    label: 'redis host'
    show_if: config.redis.internal=false
    group: 'Redis Config'
  - variable: config.redis.password
    default: ''
    description: ''
    type: password
    required: false
    label: 'redis password'
    show_if: config.redis.internal=false
    group: 'Redis Config'
  - variable: config.redis.port
    default: 6379
    description: ''
    type: int
    min: 0
    max: 50000
    required: true
    label: 'redis port'
    show_if: config.redis.internal=false
    group: 'Redis Config'
  - variable: config.redis.url
    default: ''
    description: ''
    type: string
    required: false
    label: 'redis url'
    show_if: config.redis.internal=false
    group: 'Redis Config'
  - variable: config.redis.username
    default: ''
    description: ''
    type: string
    required: false
    label: 'redis username'
    show_if: config.redis.internal=false
    group: 'Redis Config'<% } %>
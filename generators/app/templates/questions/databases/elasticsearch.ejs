<% if (_.find(databases, { name: 'elasticsearch'})) { %>

# Elasticsearch Config
  - variable: config.elasticsearch.internal
    default: true
    description: ''
    type: boolean
    required: true
    label: internal
    group: 'Elasticsearch Config'
  - variable: config.elasticsearch.version
    default: 6.4.0
    description: ''
    type: enum
    options:
      - 5.6-v1
      - 5.6.4-v1
      - 6.2-v1
      - 6.2.4-v1
      - 6.3-v1
      - 6.3.0-v1
      - 6.4
      - 6.4.0
    required: true
    label: version
    show_if: config.elasticsearch.internal=true
    group: 'Elasticsearch Config'
  - variable: config.elasticsearch.terminationPolicy
    default: Delete
    description: ''
    type: enum
    options:
      - Delete
      - Pause
      - WipeOut
      - DoNotTerminate
    required: true
    show_if: config.elasticsearch.internal=true
    label: 'termination policy'
    group: 'Elasticsearch Config'
  - variable: config.elasticsearch.pause
    default: false
    description: ''
    type: boolean
    required: true
    show_if: config.elasticsearch.internal=true&&config.elasticsearch.terminationPolicy=Pause
    label: pause
    group: 'Elasticsearch Config'
  - variable: config.elasticsearch.kibana
    default: false
    description: ''
    type: boolean
    required: true
    label: kibana
    show_if: config.elasticsearch.internal=true
    group: 'Elasticsearch Config'
  - variable: config.elasticsearch.prometheus
    default: false
    description: ''
    type: boolean
    required: true
    label: prometheus
    show_if: config.elasticsearch.internal=true
    group: 'Elasticsearch Config'
  - variable: config.elasticsearch.topology.enabled
    default: false
    description: ''
    type: boolean
    required: true
    label: 'topology enabled'
    show_if: config.elasticsearch.internal=true
    group: 'Elasticsearch Config'
  - variable: config.elasticsearch.topology.client.replicas
    default: 1
    description: ''
    type: int
    required: true
    show_if: config.elasticsearch.internal=true&&config.elasticsearch.topology.enabled=true
    label: 'client replicas'
    group: 'Elasticsearch Config'
  - variable: config.elasticsearch.topology.data.replicas
    default: 1
    description: ''
    type: int
    required: true
    show_if: config.elasticsearch.internal=true&&config.elasticsearch.topology.enabled=true
    label: 'data replicas'
    group: 'Elasticsearch Config'
  - variable: config.elasticsearch.topology.master.replicas
    default: 1
    description: ''
    type: int
    required: true
    show_if: config.elasticsearch.internal=true&&config.elasticsearch.topology.enabled=true
    label: 'master replicas'
    group: 'Elasticsearch Config'
  - variable: config.elasticsearch.replicas
    default: 1
    description: ''
    type: int
    required: true
    label: replicas
    show_if: config.elasticsearch.internal=true&&config.topology.enabled=false
    group: 'Elasticsearch Config'
  - variable: config.elasticsearch.commonConfig
    default: ''
    description: ''
    type: string
    required: false
    label: 'common config'
    show_if: config.elasticsearch.internal=true
    group: 'Elasticsearch Config'
  - variable: config.elasticsearch.dataConfig
    default: ''
    description: ''
    type: string
    required: false
    label: 'data config'
    show_if: config.elasticsearch.internal=true
    group: 'Elasticsearch Config'
  - variable: config.elasticsearch.masterConfig
    default: ''
    description: ''
    type: string
    required: false
    label: 'master config'
    show_if: config.elasticsearch.internal=true
    group: 'Elasticsearch Config'
  - variable: config.elasticsearch.host
    default: ''
    description: ''
    type: string
    required: false
    label: host
    show_if: config.elasticsearch.internal=false
    group: 'Elasticsearch Config'
  - variable: config.elasticsearch.database
    default: <%- _.snakeCase(name) %>
    description: ''
    type: string
    required: true
    label: database
    show_if: config.elasticsearch.internal=false
    group: 'Elasticsearch Config'
  - variable: config.elasticsearch.password
    default: elastic
    description: ''
    type: password
    required: true
    label: password
    show_if: config.elasticsearch.internal=false
    group: 'Elasticsearch Config'
  - variable: config.elasticsearch.port
    default: 9200
    description: ''
    type: int
    min: 0
    max: 50000
    required: true
    label: port
    show_if: config.elasticsearch.internal=false
    group: 'Elasticsearch Config'
  - variable: config.elasticsearch.url
    default: ''
    description: ''
    type: string
    required: false
    label: url
    show_if: config.elasticsearch.internal=false
    group: 'Elasticsearch Config'
  - variable: config.elasticsearch.username
    default: elastic
    description: ''
    type: string
    required: true
    label: username
    show_if: config.elasticsearch.internal=false
    group: 'Elasticsearch Config'<% } %>
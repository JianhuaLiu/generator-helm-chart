<% if (_.find(databases, { name: 'mongodb'})) { %>
  {{- $mongodb := .Values.config.mongodb }}
  mongodb_url: {{ $mongodb.url | b64enc }}
  mongodb_password: {{ $mongodb.password | b64enc }}
  {{- if .Values.config.mongo_express.enabled }}
  mongo_express_password: {{ .Values.config.mongo_express.password | b64enc }}
  {{- end }}<% } %>